---
import GlobalGraphContainer from "./GlobalGraphContainer"
---

<button onclick="window.location.href = '/?openModal=globalGraph'">
    <img src='/expand.svg' alt='graph expand'>
</button>

<dialog id="globalGraph" class="globalGraph">
    <main class="graph-container">
        <GlobalGraphContainer client:only />
    </main>
    <form method="dialog">
        <button class="close-button" onclick="window.closeModalAndGoBack('globalGraph')">Close</button> 
    </form>
</dialog>

<script>
    import "../scripts/graphModal.js";
</script>